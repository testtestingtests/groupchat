<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.ChatController">
   <children>
      <ListView fx:id="ChatListView" prefHeight="311.0" prefWidth="402.0">
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </ListView>
      <TextField fx:id="ChatTextField" layoutX="8.0" layoutY="318.0" prefHeight="26.0" prefWidth="392.0">
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </TextField>
      <Button fx:id="SendButton" layoutX="293.0" layoutY="353.0" mnemonicParsing="false" onAction="#send" prefHeight="26.0" prefWidth="94.0" text="Send">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label fx:id="LabelText" layoutX="14.0" layoutY="358.0" text="You are connected as">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
